{
  "name": "odata-filter-to-ast",
  "version": "1.0.0",
  "description": "Simple OData $filter query parser.",
  "keywords": [
    "odata",
    "filter",
    "parser"
  ],
  "homepage": "https://github.com/petrzjunior/odata-filter-to-ast",
  "bugs": "https://github.com/petrzjunior/odata-filter-to-ast/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/petrzjunior/odata-filter-to-ast.git"
  },
  "license": "MIT",
  "author": "Petr Zahradn√≠k",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/odata-filter-to-ast.cjs.js",
      "import": "./dist/odata-filter-to-ast.es.js"
    }
  },
  "main": "dist/odata-filter-to-ast.cjs.js",
  "module": "dist/odata-filter-to-ast.es.js",
  "source": "src/index.ts",
  "types": "dist/types.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "dev": "vite",
    "generate-grammar": "peggy --plugin ts-pegjs -o src/odata-grammar.ts src/odata.pegjs",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "c8": "7.11.3",
    "peggy": "1.2.0",
    "ts-pegjs": "1.2.2",
    "vite": "2.9.12",
    "vitest": "0.16.0"
  }
}
